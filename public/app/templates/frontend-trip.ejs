<!doctype html>
<html class="no-js">
<head>
  <meta charset="utf-8">
  <title><%= page.title %></title>
  <meta name="viewport" content="width=device-width">
  <% include common/styles %>
  <style>
  .schedule-full ul {
    margin-left: 20px;
  }

  .schedule-full > li {
    min-height: 120px;
    margin-bottom: 10px;
  }

  .schedule-full ul > li {
    padding: 0;
  }
  </style>
